            <TextArea fx:id="contentTextArea" editable="false" prefHeight="362.0" prefWidth="600.0" />
            <HBox alignment="CENTER" prefHeight="35.0" prefWidth="599.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="35.0" prefWidth="86.0">
